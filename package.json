  "license": "MIT",
  "scripts": {
    "start": "tsc -p . --noImplicitAny && echo *build done* &&node --enable-source-maps ./build/main.js",
    "build": "tsc -p . --noImplicitAny",
    "lint": "eslint --ignore-path .gitignore 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --ignore-path .gitignore 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:dbpush": "prisma db push",
    "check": "tsc -p . --noEmit",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "engines": {
    "node": ">=16.14"
  },
  "dependencies": {
    "@prisma/client": "4.7.1",
    "argon2": "^0.30.3",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "typescript": "^4.4.4",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitest/ui": "^0.27.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "vitest": "^0.27.0"
  }
